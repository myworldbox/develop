{
    "email": "^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)*(\\.[a-zA-Z]{2,})$",
    "url": "^(http(s)?:\\/\\/)?[\\w.-]+\\.[a-zA-Z]{2,}(\\/\\S*)?$",
    "password": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)[a-zA-Z\\d]{8,}$",
    "number": "^\\d+$",
    "date": "^(0[1-9]|1[0-2])\\/(0[1-9]|1\\d|2\\d|3[01])\\/(19|20)\\d{2}$",
    "ip": "^(\\d{1,3}\\.){3}\\d{1,3}$"
}
